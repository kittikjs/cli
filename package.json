{
  "name": "kittik-cli",
  "version": "0.0.0-semantic-release",
  "description": "CLI tool for starting Kittik presentations",
  "author": "ghaiklor",
  "homepage": "https://github.com/kittikjs/cli#readme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kittikjs/cli/issues"
